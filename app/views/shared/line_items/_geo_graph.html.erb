<tr>
    <td><%= link_to geo_graph.name, geo_graph_path(geo_graph) %></td>
    <td><%= geo_graph.updated_at.to_formatted_s(:long_ordinal) %></td>
    <td>
    <% if current_user == geo_graph.user %>
        <%= link_to 'Edit', edit_geo_graph_path(geo_graph) %>
        <%= link_to 'Delete', geo_graph_path(geo_graph), :method => :delete, :confirm => t('.confirm', :geo_graph => GeoGraph.model_name.human.downcase) %>
    <% end %>
    </td>
</tr>
