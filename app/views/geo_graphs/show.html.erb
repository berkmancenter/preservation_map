<h1>GeoGraph#show</h1>
<table>
    <% @place_measures.each_with_index do |pm, i| %>
        <% if i == 0 %>
            <tr><th>Library Name</th>
            <% @geograph.measures.each do |measure| %>
                <th><%= measure.name %></th>
            <% end %>
            </tr>
        <% end %>
        <% if i == 0 or @place_measures[i-1].place_id != pm.place_id %>
            </tr><tr><td><%= pm.place.name %></td>
        <% end %>
        <td><%= pm.value %></td>
<% end %>
</table>
<div id="map" style="width: 500px; height: 500px;"></div>
<%= javascript_include_tag('http://maps.google.com/maps/api/js?v=3.5&sensor=false') %>
<%= javascript_include_tag('openlayers/OpenLayers') %>
<%= javascript_include_tag params[:controller] + '_' + params[:action] %>
